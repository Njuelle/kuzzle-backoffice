{
  "name": "kuzzle-bo",
  "version": "0.6.0",
  "description": "Official back office for manage and monitor your Kuzzle",
  "main": "app.js",
  "scripts": {
    "test": "rm -f ./features/errorShots/*; grunt linter && ./node_modules/.bin/wdio ./features/wdio.conf.js",
    "test-selective": "rm -f ./features/errorShots/*; grunt linter && ./node_modules/.bin/wdio ./features/wdio.conf.js --cucumberOpts.tags @myTest",
    "start": "node ./bin/www",
    "dev": "webpack-dev-server --history-api-fallback --inline --progress --public localhost"
  },
  "keywords": [
    "kuzzle",
    "backoffice"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kuzzleio/kuzzle-bo.git"
  },
  "author": "Kuzzle Team",
  "license": "Apache-2.0",
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-handlebars": "^2.0.1",
    "kuzzle-sdk": "1.7.1",
    "lodash": "^3.10.1",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0",
    "socket.io": "^1.3.7"
  },
  "devDependencies": {
    "async": "^1.5.0",
    "cucumber": "^0.5.3",
    "grunt": "^0.4.5",
    "grunt-contrib-jshint": "^0.12.0",
    "grunt-contrib-sass": "^0.9.2",
    "grunt-contrib-watch": "^0.6.1",
    "gruntify-eslint": "^2.0.0",
    "q": "^1.4.1",
    "request": "^2.67.0",
    "webdriverio": "^3.4.0",
    "autoprefixer": "^6.0.3",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^2.7.1",
    "node-libs-browser": "^1.0.0",
    "null-loader": "^0.1.1",
    "postcss-loader": "^0.8.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.0",
    "rimraf": "^2.5.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
