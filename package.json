{
  "name": "kuzzle-bo",
  "version": "1.0.0-RC5",
  "description": "Official back office for manage and monitor your Kuzzle",
  "main": "app.js",
  "scripts": {
    "build": "./build.sh",
    "test": "rm -f ./features/errorShots/*; ./node_modules/.bin/wdio ./features/wdio.conf.js",
    "test-selective": "rm -f ./features/errorShots/*; ./node_modules/.bin/wdio ./features/wdio.conf.js --cucumberOpts.tags @myTest",
    "start": "./bin/www",
    "dev": "NODE_ENV=dev node ./bin/www"
  },
  "keywords": [
    "kuzzle",
    "backoffice"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kuzzleio/kuzzle-bo.git"
  },
  "author": "Kuzzle Team",
  "license": "Apache-2.0",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "syntax-async-functions",
      "transform-regenerator"
    ]
  },
  "dependencies": {
