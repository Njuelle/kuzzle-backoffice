{
  "admin": {
    "indexes": {
      "_canCreate": true,
      "*": {
      "_canDelete": true,
      "collections": {
        "_canCreate": true,
        "*": {
          "_canDelete": true,
          "controllers": {
            "*": {
              "actions": {
                "*": true
                }
              }
            }
          }
        }
      },
      "%kuzzle": {
        "_canDelete": false,
        "collections": {
          "_canCreate": true,
          "*": {
            "_canDelete": false,
            "controllers": {
              "*": {
                "actions": {
                  "*": true
                  }
                }
              }
            }
          }
      }
    }
  },
  "anonymous": {
    "indexes": {
      "_canCreate": false,
      "*": {
        "_canDelete": false,
        "collections": {
          "_canCreate": false,
          "*": {
            "_canDelete": false,
            "controllers": {
              "auth": {
                "actions": {
                  "login": true,
                  "checkToken": true,
                  "getCurrentUser": true
                }
              }
            }
          }
        }
      }
    }
  },
  "default": {
    "indexes": {
      "_canCreate": false,
      "*": {
        "_canDelete": false,
        "collections": {
          "_canCreate": false,
          "*": {
            "_canDelete": false,
            "controllers": {
              "auth": {
                "actions": {
                  "logout": true,
                  "checkToken": true,
                  "getCurrentUser": true
                }
              }
            }
          }
        }
      }
    }
  }  
}