<div class="row edit-document" ng-controller="StorageAddCtrl" ng-init="init()">
    <div class="col-sm-12 headline">
        <h1 class="page-header">
            Storage <small>add</small>
        </h1>
    </div>

    <div class="col-sm-10 col-sm-offset-1 ">
        <json-editor class="form" schema="schema" startval="document" buttons-controller="StorageAddButtonsCtrl">
            <button type="submit" class="btn btn-success control" ng-click="onSubmit($event)">Add</button>
            <a class="control" ui-sref="storage.browse.documents({collection: collection})">Cancel</a>
        </json-editor>
    </div>
</div>